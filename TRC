import random
people = ['Dawid', 'Arturt', 'Samuel',
          'Pawel', 'Dawid2', 'Maciej ',
          'Mateusz', 'Mateusz2', 'Adam','Jakub',
          'Filip', 'Jakub2', 'Grzegorz',
          'Jan', 'Sebastian', 'Marcink', 'Michal2',
          'Piotr', 'Rafa≈Ç']
teamA = []
teamB = []
for i in range(5):
    one = random.choice(people)
    teamA.append(one)
    people.remove(one)
for i in range(5):
    one = random.choice(people)
    teamB.append(one)
    people.remove(one)
